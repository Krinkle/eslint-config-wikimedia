{
  "extends": "./not-es2015.json",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "es6": true
  },
  "rules": {
    "arrow-parens": "error",
    "no-misleading-character-class": "error",
    "no-new-require": "error",
    "no-useless-computed-key": "error",
    "one-var": "off",
    "template-curly-spacing": ["error", "always"],
    "vars-on-top": "off"
  }
}
