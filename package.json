{
	"name": "eslint-config-wikimedia",
	"version": "0.15.3",
	"description": "ESLint config following Wikimedia code conventions.",
	"main": "common.json",
	"scripts": {
		"test": "npm run lint && node test/test.js",
		"lint": "eslint . --ext .js,.json,.vue --max-warnings 0",
		"build-invalid": "node test/test.js dump"
	},
	"repository": {
		"type": "git",
