{
  "name": "eslint-config-wikimedia",
  "version": "0.12.0",
  "description": "ESLint config following Wikimedia code conventions.",
  "main": "common.json",
  "scripts": {
    "test": "eslint --report-unused-disable-directives . --ext .js,.json && node test/test.js",
    "build-invalid": "node test/test.js dump"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wikimedia/eslint-config-wikimedia.git"
  },
  "homepage": "https://www.mediawiki.org/wiki/Manual_talk:Coding_conventions/JavaScript",
  "bugs": "https://github.com/wikimedia/eslint-config-wikimedia/issues",
  "keywords": [
    "style guide",
    "eslint",
    "eslintconfig",
    "wikimedia"
  ],
  "files": [
    "common.json",
    "client.json",
    "node.json",
    "jquery.json",
    "qunit.json",
    "server.json",
    "language/es5.json",
    "language/es6.json",
    "language/es2016.json",
    "language/es2017.json",
    "language/rules-es5.json",
    "language/rules-es6.json",
    "language/rules-es2016.json",
    "language/rules-es2017.json",
    "language/not-es5.json",
    "language/not-es6.json",
    "language/not-es2016.json",
    "language/not-es2017.json",
    "language/es2015.json"
  ],
  "contributors": [
    "James D. Forrester <jforrester@wikimedia.org>",
    "Ed Sanders <ejsanders@gmail.com>",
    "Timo Tijhof <krinklemail@gmail.com>",
    "Oleg Gaidarenko <markelog@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-no-jquery": "^2.1.0",
    "eslint-plugin-qunit": "^4.0.0"
  },
  "devDependencies": {
    "assert-diff": "^2.0.3"
  }
}
