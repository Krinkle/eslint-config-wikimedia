{
  "name": "eslint-config-wikimedia",
  "version": "0.6.0",
  "description": "ESLint config following Wikimedia code conventions.",
  "main": ".eslintrc.json",
  "scripts": {
    "lint": "bin/eslint-wikimedia --ignore-pattern test/fixtures/invalid\\* .",
    "lint:fix": "npm run -s lint -- --fix",
    "test": "npm run -s lint && node test/testValid.js && node test/testInvalid.js",
    "build-invalid": "node test/testInvalid.js dump > test/fixtures/invalid-results.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wikimedia/eslint-config-wikimedia.git"
  },
  "homepage": "https://www.mediawiki.org/wiki/Manual_talk:Coding_conventions/JavaScript",
  "bugs": "https://github.com/wikimedia/eslint-config-wikimedia/issues",
  "keywords": [
    "style guide",
    "eslint",
    "eslintconfig",
    "wikimedia"
  ],
  "bin": {
    "eslint-wikimedia": "bin/eslint-wikimedia"
  },
  "files": [
    ".eslintrc.json"
  ],
  "author": "Gaidarenko Oleg <markelog@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "eslint-plugin-qunit": "^3.2.1"
  },
  "devDependencies": {
    "assert-diff": "^1.2.0",
    "eslint": "^5.0.0",
    "eslint-plugin-qunit": "^3.2.1"
  }
}
