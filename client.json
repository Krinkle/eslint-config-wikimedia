{
	"plugins": [ "es" ],
	"extends": [
		"./common",
		"./language/es5"
	],
	"env": {
		"browser": true
	},
	"rules": {
		"no-alert": "error",
		"no-console": "error",
		"no-implied-eval": "error",
		"no-restricted-properties": [
			"error",
			{
				"property": "parentElement",
				"message": "Prefer parentNode to parentElement as Node.parentElement is not supported by IE11."
			}
		]
	}
}
